{
  "name": "xling",
  "version": "0.7.3",
  "description": "Unified CLI for managing AI tool settings",
  "homepage": "https://github.com/kingsword09/xling/tree/main#readme",
  "bugs": {
    "url": "https://github.com/kingsword09/xling/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kingsword09/xling.git"
  },
  "type": "module",
  "files": [
    "dist",
    "dist/ui"
  ],
  "bin": {
    "xling": "./dist/run.js"
  },
  "scripts": {
    "build": "tsdown && bun run build:ui",
    "dev": "tsdown --watch",
    "build:ui": "vite build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint src",
    "lint:fix": "oxlint --fix src",
    "format": "oxfmt src",
    "format:check": "oxfmt --check src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "ai",
    "cli",
    "settings",
    "claude",
    "codex",
    "gemini"
  ],
  "author": "Kingsword kingsword09 <kingsword09@gmail.com>",
  "license": "Apache-2.0",
  "packageManager": "bun@1.3.2",
  "engines": {
    "node": ">=20.19.0"
  },
  "oclif": {
    "bin": "xling",
    "dirname": "xling",
    "commands": "./dist/commands",
    "topicSeparator": ":"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@types/bun": "^1.3.2",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "oclif": "^4.22.44",
    "oxfmt": "^0.13.0",
    "oxlint": "^1.28.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "tsdown": "^0.16.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.27",
    "@oclif/core": "^4.8.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "ai": "^5.0.93",
    "class-variance-authority": "^0.7.1",
    "cli-table3": "^0.6.5",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "open": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "smol-toml": "^1.4.2",
    "streamdown": "^1.5.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  }
}
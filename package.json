{
  "name": "xling",
  "version": "0.10.1",
  "description": "Unified CLI for AI tools: settings management, API proxy with load balancing, multi-model discussions",
  "homepage": "https://github.com/kingsword09/xling/tree/main#readme",
  "bugs": {
    "url": "https://github.com/kingsword09/xling/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kingsword09/xling.git"
  },
  "type": "module",
  "files": [
    "dist",
    "dist/ui"
  ],
  "bin": {
    "xling": "./dist/run.js"
  },
  "scripts": {
    "build": "tsdown && bun run build:ui",
    "dev": "tsdown --watch",
    "manifest": "bunx oclif manifest",
    "schema": "bun run ./scripts/generate-schema.ts",
    "build:ui": "vite build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint --type-aware --type-check",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "ai",
    "cli",
    "settings",
    "claude",
    "codex",
    "gemini"
  ],
  "author": "Kingsword kingsword09 <kingsword09@gmail.com>",
  "license": "Apache-2.0",
  "packageManager": "bun@1.3.4",
  "engines": {
    "node": ">=20.19.0"
  },
  "oclif": {
    "bin": "xling",
    "dirname": "xling",
    "commands": "./dist/commands",
    "topicSeparator": ":",
    "topics": {
      "settings": {
        "description": "Manage AI tool settings (Claude, Codex, Gemini, xling)"
      },
      "git": {
        "description": "Git workflow utilities (PR management, worktrees)"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.15",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/bun": "^1.3.4",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "oclif": "^4.22.54",
    "oxfmt": "^0.13.0",
    "oxlint": "^1.32.0",
    "oxlint-tsgolint": "^0.8.5",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "tsdown": "^0.16.8",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.28",
    "@oclif/core": "^4.8.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "ai": "^5.0.108",
    "class-variance-authority": "^0.7.1",
    "cli-table3": "^0.6.5",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "open": "^11.0.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "smol-toml": "^1.5.2",
    "streamdown": "^1.6.10",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  }
}

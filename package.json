{
  "name": "xling",
  "version": "0.7.0",
  "description": "Unified CLI for managing AI tool settings",
  "homepage": "https://github.com/kingsword09/xling/tree/main#readme",
  "bugs": {
    "url": "https://github.com/kingsword09/xling/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kingsword09/xling.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "xling": "./dist/run.js"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint src",
    "lint:fix": "oxlint --fix src",
    "format": "oxfmt src",
    "format:check": "oxfmt --check src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "ai",
    "cli",
    "settings",
    "claude",
    "codex",
    "gemini"
  ],
  "author": "Kingsword kingsword09 <kingsword09@gmail.com>",
  "license": "Apache-2.0",
  "packageManager": "bun@1.3.2",
  "engines": {
    "node": ">=20.19.0"
  },
  "oclif": {
    "bin": "xling",
    "dirname": "xling",
    "commands": "./dist/commands",
    "topicSeparator": ":"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.14",
    "@types/bun": "^1.3.2",
    "@types/node": "^24.10.0",
    "oclif": "^4.22.44",
    "oxfmt": "^0.13.0",
    "oxlint": "^1.28.0",
    "tsdown": "^0.16.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.27",
    "@oclif/core": "^4.8.0",
    "ai": "^5.0.93",
    "cli-table3": "^0.6.5",
    "smol-toml": "^1.4.2",
    "zod": "^4.1.12"
  }
}
